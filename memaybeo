coroutine.wrap(PKOJEH_fake_script)()
local function IGLVYX_fake_script() -- back.LocalScript 
	local script = Instance.new('LocalScript', back)

	local m = script.Parent.Parent.Parent.PickScript
	
	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.bad.Visible = false
		script.Parent.Parent.bad.source.Visible = false
		script.Parent.Visible = false
		script.Parent.Parent.clear.Visible = false
		script.Parent.Parent.execute.Visible = false
		script.Parent.Parent.outputfake.Visible = false
		script.Parent.Parent.title.Visible = false
		script.Parent.Parent.ver.Visible = false
		script.Parent.Parent.logsopen.Visible = false
		
		local object = script.Parent.Parent
		object.Position = UDim2.new(0.5, 0, 0.5, 0)
	
		--When the game starts, it will wait 2 seconds, then make the tween.
		object:TweenSize(UDim2.new(0, -3, 0, -3), Enum.EasingDirection.Out, Enum.EasingStyle.Quint)
		wait(1)
		script.Parent.Parent.Visible = false
		
		m.aaaa.Visible = true
		m.TextButton.Visible = true
		m.TextLabel.Visible = true
		m.Size = UDim2.new(0, 457,0, 247)
		
		m.Visible = true
	end)
end
coroutine.wrap(IGLVYX_fake_script)()
local function JQSXVIK_fake_script() -- Nerdie.load 
	local script = Instance.new('LocalScript', Nerdie)

	if script.Parent.Visible == true then
		local object = script.Parent.Parent
		object.Position = UDim2.new(0.5, 0, 0.5, 0)
	
		--When the game starts, it will wait 2 seconds, then make the tween.
		wait(1)
		object:TweenSize(UDim2.new(0, -3, 0, -3), Enum.EasingDirection.Out, Enum.EasingStyle.Quint)
	end
end
coroutine.wrap(JQSXVIK_fake_script)()
local function TKYS_fake_script() -- Nerdie.print 
	local script = Instance.new('LocalScript', Nerdie)

	local function CUDAI2MET()
		print("New Furture.")
		print("UI darker Colour")
		print("New Position for the Script Opening!")
		print("Haven't added the Close's Animation so stay patient!")
		print("Thanks for using my script!")
		print("Later will have Credits UI, Save file and Open files!")
		print("neither it will takes a long time so anyways stay patient.")
		print("{Made by Tuan Anh (Nerdie), Nerdie on Top!}")
	end
	
	-- New Furture.
	-- UI darker Colour
	-- New Position for the Script Opening!
	-- Haven't added the Close's Animation so stay patient!
	-- Thanks for using my script!
	-- Later will have Credits UI, Save file and Open files!
