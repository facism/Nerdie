-- Scripts:

local function UHZEBSQ_fake_script() -- execute.but0 
	local script = Instance.new('LocalScript', execute)

	local startSize = UDim2.new(0, 104,0, 28)
	local hoverSize = UDim2.new(0, 88,0, 28)
	
	script.Parent.MouseEnter:Connect(function()
		script.Parent:TweenSize(startSize,Enum.EasingDirection.Out,Enum.EasingStyle.Sine,.25,true)
	end)
	
	script.Parent.MouseLeave:Connect(function()
		script.Parent:TweenSize(hoverSize,Enum.EasingDirection.Out,Enum.EasingStyle.Sine,.25,true)
	end)
end
coroutine.wrap(UHZEBSQ_fake_script)()
local function ELXG_fake_script() -- execute.executescript 
	local script = Instance.new('LocalScript', execute)

	local v = script.Parent.Parent.bad.source
	
	script.Parent.MouseButton1Click:Connect(function()
		loadstring(v.Text)()
	end)
end
coroutine.wrap(ELXG_fake_script)()
local function EERN_fake_script() -- clear.but 
	local script = Instance.new('LocalScript', clear)

	local startSize = UDim2.new(0, 104,0, 28)
	local hoverSize = UDim2.new(0, 88,0, 28)
	
	script.Parent.MouseEnter:Connect(function()
		script.Parent:TweenSize(startSize,Enum.EasingDirection.Out,Enum.EasingStyle.Sine,.25,true)
	end)
	
	script.Parent.MouseLeave:Connect(function()
		script.Parent:TweenSize(hoverSize,Enum.EasingDirection.Out,Enum.EasingStyle.Sine,.25,true)
	end)
end
coroutine.wrap(EERN_fake_script)()
local function UBYORN_fake_script() -- clear.clearscript 
	local script = Instance.new('LocalScript', clear)

	local v = script.Parent.Parent.bad.source
	
	script.Parent.MouseButton1Click:Connect(function()
		v.Text = ""
	end)
end
coroutine.wrap(UBYORN_fake_script)()
