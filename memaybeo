local function MAQIPOJ_fake_script() -- keycorrect.LocalScript 
	local script = Instance.new('LocalScript', keycorrect)

	
	if script.Parent.Parent.key.Text == "god" or "God" or "turanc" or "nerdie" or "signupredir on top" then
		script.Parent.MouseButton1Click:Connect(function()
			script.Parent.Visible = false
			script.Parent.Parent.key.Visible = false
			script.Parent.Parent.akey.Visible = false
			script.Parent.Parent.makingsure.Visible = false
			script.Parent.Parent.ver.Visible = false
			script.Parent.Parent.TextButton.Visible = true
			script.Parent.Parent.TextLabel.Visible = true
			script.Parent.Parent.aaaa.Visible = true
		end)
	end
end
