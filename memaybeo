local function CNKB_fake_script() -- aaaa.big button 
	local script = Instance.new('LocalScript', aaaa)

	local startsize = UDim2.new(script.Parent.Size.Width.Scale * 0, 91, script.Parent.Size.Height.Scale * 0, 92)
	
	local hoversize = UDim2.new(script.Parent.Size.Width.Scale * 0, 119, script.Parent.Size.Height.Scale * 0, 119) --change 1.05 to whatever works for you
	
	script.Parent.MouseEnter:Connect(function()
		script.Parent:TweenSize(hoversize,Enum.EasingDirection.Out,Enum.EasingStyle.Sine,.25,true)
	end)
	
	script.Parent.MouseLeave:Connect(function()
		script.Parent:TweenSize(startsize,Enum.EasingDirection.Out,Enum.EasingStyle.Sine,.25,true)
	end)
	
	-- {0, 113},{0, 93}
	-- {0, 82},{0, 76}
	-- {0, 82},{0, 76}
end
coroutine.wrap(CNKB_fake_script)()
local function VFXGSOV_fake_script() -- aaaa.LocalScript 
	local script = Instance.new('LocalScript', aaaa)

	script.Parent.MouseButton1Click:Connect(function()
		local a = script.Parent.Parent.TextLabel
		a.Visible = false
		script.Parent.Visible = false
		script.Parent.Parent.TextButton.Visible = false
		
		local object = script.Parent.Parent
		object.Position = UDim2.new(0.5, 0, 0.5, 0)
	
		--When the game starts, it will wait 2 seconds, then make the tween.
		object:TweenSize(UDim2.new(0, -3, 0, -3), Enum.EasingDirection.Out, Enum.EasingStyle.Quint)
		wait(1)
		-- THE VISIBLE CLAN
		script.Parent.Parent.Parent.Nerdie.Visible = true
		script.Parent.Parent.Parent.Nerdie.Size = UDim2.new(0, 537,0, 271)
		script.Parent.Parent.Parent.Nerdie.bad.Visible = true
		script.Parent.Parent.Parent.Nerdie.bad.source.Visible = true
		script.Parent.Parent.Parent.Nerdie.clear.Visible = true
		script.Parent.Parent.Parent.Nerdie.execute.Visible = true
		script.Parent.Parent.Parent.Nerdie.outputfake.Visible = true
		script.Parent.Parent.Parent.Nerdie.title.Visible = true
		script.Parent.Parent.Parent.Nerdie.ver.Visible = true
		script.Parent.Parent.Parent.Nerdie.back.Visible = true
		
		script.Parent.Parent.Visible = false
	end)
	
	local function MADE_BY_TUANANH()
		print("SAY HI TO MY SCRIPT!!")
		print("TUAN ANH ON TOP!")
		print("NERDIE IS ON THE LEADER?!?!")
		print("You didn't have to pay for this so say thanks!")
	end
	
	MADE_BY_TUANANH()
end
coroutine.wrap(VFXGSOV_fake_script)()
